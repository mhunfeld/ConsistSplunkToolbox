<form script="components/timerange-slider/examples/timerangeSlider_main.js" theme="light">
  <label>Timerange Slider</label>
  <init>
    <set token="show">true</set>
  </init>
  <description>Timerange Slider Input</description>
  <fieldset submitButton="true" autoRun="false"></fieldset>
  <row>
    <panel id="customFieldset">
      <title>Timerange Slider</title>
      <input id="timeUnit" type="dropdown" token="timeUnit_o">
        <label>Zeiteinheit</label>
        <choice value="m">Minuten</choice>
        <choice value="h">Stunden</choice>
        <choice value="d">Tage</choice>
        <default>Stunden</default>
        <change>
          <set token="labelSuffix">$label$ </set>
          <set token="timeUnit">$value$</set>
        </change>
      </input>
      <html>
        <!-- Anker fÃ¼r JS-Component-->
        <div id="timeRangeSlider"></div>

        <!-- Timeslider als WebComponent-->
        <co-timerange-slider id="webComponentSlider" token="timeAsComponent_tok" searchWhenChanged="true" depends="?show?">
              <label>Als WebComponent</label>
              <min>0</min>
              <max>96</max>
              <step>12</step>
              <defaultValue>24</defaultValue>
              <prefix>-@</prefix>
              <suffix>?timeUnit?</suffix>
              <labelPrefix>letzte </labelPrefix>
              <labelSuffix>?labelSuffix?</labelSuffix>
              <parentElement>customFieldset</parentElement>
          </co-timerange-slider>
        </html>
    </panel>
  </row>
</form>