<form script="components/timerange-slider/examples/timerangeSlider_main.js" theme="dark">
  <label>Timerange Slider</label>
  <description>Timerange Slider Input</description>
  <fieldset submitButton="true" autoRun="false"></fieldset>
  <row>
    <panel>
      <title>Timerange Slider with JS</title>
      <input id="timerange1" type="time" token="time1_tok">
        <label>Zeitraum 1</label>
        <default>@24h</default>
      </input>
    </panel>
  </row>
  <row>
    <panel id="customFieldset">
      <title>Timerange Slider as Web Component</title>
      <input id="timerange2" type="text" token="time2_tok">
        <label>Zeitraum 2</label>
        <default>@24h</default>
      </input>
      <html>
          <co-timerange-slider id="webComponentSlider" token="timeAsComponent_tok">
              <label>Als WebComponent</label>
              <min>0</min>
              <max>96</max>
              <step>12</step>
              <defaultValue>24</defaultValue>
              <prefix>@</prefix>
              <suffix>h</suffix>
              <labelPrefix>letzte </labelPrefix>
              <labelSuffix> Stunden</labelSuffix>
              <parentElement>customFieldset</parentElement>
          </co-timerange-slider>
        </html>
    </panel>
  </row>
</form>